{
  "name": "fireblocks-netlink-v2-api-validator",
  "version": "0.3.1",
  "description": "Fireblocks Network Link v2 API Validator",
  "author": "Fireblocks",
  "license": "Apache-2.0",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/fireblocks/fireblocks-network-link"
  },
  "main": "dist",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "server": "NODE_ENV=development ts-node src/server/server.ts | pino-pretty",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "test": "NODE_ENV=test jest --verbose --json --outputFile=test-results.json",
    "test:watch": "npm run test -- --watch",
    "test:sanity": "npm run test -- --config=jest.sanity.config.ts tests/sanity-tests",
    "test:sanity:watch": "npm run test:sanity -- --watch",
    "test:self": "npm run test -- --config=jest.sanity.config.ts tests/self-tests",
    "test:self:watch": "npm run test:self -- --watch",
    "test:server": "npm run test -- tests/server-tests",
    "test:server:watch": "npm run test:server -- --watch",
    "generate-rsa-key-pair": "ts-node generate-signing-keys.ts rsa",
    "generate-ecdsa-secp256k1-key-pair": "ts-node generate-signing-keys.ts ecdsa-secp256k1",
    "generate-ecdsa-prime256v1-key-pair": "ts-node generate-signing-keys.ts ecdsa-prime256v1",
    "generate-openapi-client": "(cd ../openapi; ./lint-openapi.sh) && NODE_ENV=development ts-node generate-openapi-client.ts",
    "make-unified-openapi": "NODE_ENV=development ts-node make-unified-openapi.ts",
    "build-docs": "npx @redocly/cli build-docs ../openapi/fb-unified-openapi.yaml --output ../openapi/docs.html"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.4.0",
    "bignumber.js": "^9.1.1",
    "bs58": "^5.0.0",
    "convict": "^6.2.4",
    "dotenv": "^16.3.1",
    "fastify": "^4.19.2",
    "fastify-openapi-glue": "^2.7.2 ",
    "form-data": "^4.0.0",
    "hi-base32": "^0.5.1",
    "http-status-codes": "^2.2.0",
    "jest-extended": "^4.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.1",
    "randexp": "^0.5.3",
    "uuid": "^9.0.0",
    "verror": "^1.10.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.6.1",
    "jest-expect-message": "^1.1.3",
    "jest-extended": "^4.0.0",
    "jest-html-reporters": "^3.1.4",
    "json-schema-faker": "^0.5.3",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "type-fest": "^0.20.2",
    "typescript": "^4.9.5"
  }
}